
NewVisitor(filename, packageName, parserName, superclass, importTxt) ::= <<
/** Generated by XVisitorDT */
xvisitor grammar <filename>;

options {
	parserClass = <parserName>;
	<if(superclass)>superClass = <superclass>;<endif>
}

@header {
	package <packageName>;

	<if(importTxt)>import <importTxt>;<endif>
}

<filename>Main
	: rule
	;

rule
	: /xpathSpec
		{ ruleAction(); }
	;

>>
